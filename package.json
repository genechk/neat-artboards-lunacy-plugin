{
  "name": "neat-artboards",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Neat Artboards",
    "manifest": "src/manifest.json",
    "main": "neat-artboards.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "printWidth": 75,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "license": "GPLv3",
  "author": "genechk <genek.tomashko@gmail.com>"
}
